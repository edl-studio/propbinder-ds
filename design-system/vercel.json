{
  "buildCommand": "npm run build:deploy",
  "outputDirectory": "deploy",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": null,
  "headers": [
    {
      "source": "/storybook/(.*\\.js)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    },
    {
      "source": "/storybook/(.*\\.css)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/storybook",
      "destination": "/storybook/index.html",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/app-shell-preview",
      "destination": "/app-shell-preview.html"
    },
    {
      "source": "/((?!storybook|app-shell-preview|public|Brockmann|Assets|Dummy-avatars|favicon.ico|propbinder-white.svg|.*\\.js|.*\\.css).*)",
      "destination": "/index.html"
    }
  ]
}
